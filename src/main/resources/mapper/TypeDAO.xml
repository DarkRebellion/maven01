<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.dao.TypeDAO">
    <select id="getalltype" resultType="com.neuedu.bean.TypeBean">
        select * from typetable;
    </select>
    <select id="select" resultType="com.neuedu.bean.TypeBean">
        select * from typetable where tid=#{tid};
    </select>
</mapper>